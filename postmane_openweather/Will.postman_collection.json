						"exec": [
							"pm.test(\"Latitude is 51.51 and Lon is -0.13\", () => {",
							"    const responseJson = pm.response.json()",
							"    pm.expect(responseJson.coord.lat).to.eql(51.51)",
							"    pm.expect(responseJson.coord.lon).to.eql(-.13)",
							"});",
							"pm.test(\"temp value is in between temp max and temp min\", () => {",
							"    const responseJson = pm.response.json()",
							"    pm.expect(responseJson.main.temp).to.be.gt(responseJson.main.temp_min).and.to.be.lt(responseJson.main.temp_max)",
							"});",
							"",
							"pm.test(\"Name is London\", () => {",
							"    const responseJson = pm.response.json()",
							"    pm.expect(responseJson.name).to.eql(\"London\")",
							"});",
							"pm.test(\"Code is 200 OK\", () => {",
							"    pm.response.to.have.status(200)",
							"});"
